{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "AKSPerf": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "ProcessorInformation",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/ProcessorInformation",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:443;http://+:80",
        "ASPNETCORE_HTTPS_PORT": "44368"
      },
      "httpPort": 10000,
      "useSSL": true,
      "sslPort": 44368
    }
  }
}